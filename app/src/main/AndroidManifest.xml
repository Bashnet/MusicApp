<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.music_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".libraryActivity"
            android:label="Library"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RecentlyPlayedActivity"
            android:label="Recently played"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".favouritesActivity"
            android:label="Favourites"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".playlistActivity"
            android:label="Playlist"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".artistsActivity"
            android:label="Artists"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".albumsActivity"
            android:label="Albums"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".songsActivity"
            android:label="Songs"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".genresActivity"
            android:label="Genres"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NowPlayingActivity"
            android:label="Now Playing"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>